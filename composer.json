{
  "name": "laravel/laravel",
  "description": "The Alive Mod Website & War Room",
  "keywords": [
    "ALiVE",
    "ARMA3"
  ],
  "license": "MIT",
  "require": {
    "laravel/framework": "4.0.*",
    "doctrine/couchdb": "1.0.0-alpha1",
    "cartalyst/sentry": "2.0.*",
    "prologue/alerts": "0.3.0",
    "laravelbook/ardent": "2.1.0",
    "way/generators": "~1.1",
    "codesleeve/stapler": "1.0.0-Beta4",
    "anlutro/curl": "0.1.1",
    "webpatser/laravel-countries": "~1.3.4",
    "mandrill/mandrill": "1.0.*",
    "thomaswelton/laravel-gravatar": "~0.1.0"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-0": {
      "Alive": "app/lib",
      "Tempo": "app/lib"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
